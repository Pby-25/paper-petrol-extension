{
    "name": "PaperPetrol",
    "version": "0.0.1",
    "description": "Extension for PaperPetrol.com",
    "permissions": [
      "storage", 
      "tabs",
      "*://*.google.com/maps/*",
      "*://localhost/*",
      "*://*.paperpetrol.com/*"
      ],
    "browser_action": {
        "default_title": "PaperPetrol.com"
      },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "manifest_version": 2
  }